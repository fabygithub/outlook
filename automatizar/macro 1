'ingresa un valor

 Sub Cells_Example()
  Cells(1, 1).Value = "Hello"
 End Sub

(1,1) = fila 1 , columna 1

'va sumando de a 1 por las filas a

Sub loopcell()
 Dim i As Integer
   For i = 1 To 10
     Cells(i, 1).Value = i
     Next i
End Sub

